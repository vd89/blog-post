<!-- @format -->

<div class="jumbotron">
	<div class="container">
		<h1 class="display-3">Hello, From Dax</h1>
		<p class="lead">
			The other change in the private and public
		</p>
		<hr class="my-4" />
		<p>
			There are some change that i have done in my private git repos and they
			are been rendered here
		</p>
		<p class="lead">
			<a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a>
		</p>
	</div>
</div>
<div class="container">
	<%if (blogs.length >0){%>
	<div class="row">
		<div class="card-group">
			<%blogs.forEach(blog=>{ %>
			<div class="col-md-4">
				<div class="card">
					<img
						src="./images/<%= blog.blogImage%> "
						class="card-img-top"
						alt="blog image "
					/>
				</div>
				<div class="card-body">
					<h5 class="card-title"><h3><%= blog.title%></h3></h5>
					<p class="card-text"><%= blog.snippet%></p>
					<p class="card-text">
						<small class="text-muted"
							>Posted By <%= blog.userName%> <%=
							blog._id.getTimestamp().toDateString()%></small
						>
					</p>
					<p>
						<a href="/blog/<%= blog._id%>" class="btn btn-secondary">View</a>
					</p>
				</div>
			</div>
			<%})%>
		</div>
	</div>

	<%}else{%>
	<p>there is nothing to dispay</p>

	<%}%>
</div>
